\n        <TimerComp v-if=\"rendered\" :paused=\"paused || completed\" />\r\n\r\n        <button v-if=\"!playing\" class=\"action-btn d-block\" @click=\"startGame\">\r\n          Start Game\r\n        </button>\r\n        <button v-if=\"playing && paused && !completed\"\r\n                class=\"action-btn d-block\" @click=\"resumeGame\">\r\n          Resume Game\r\n        </button>\r\n        <button v-if=\"playing && !paused && !completed\"\r\n                class=\"action-btn d-block\" @click=\"pauseGame\">\r\n          Pause Game\r\n        </button>\r\n        <button v-if=\"completed\" class=\"action-btn d-block\" @click=\"enterScore()\">\r\n          Enter your score\r\n        </button>\r\n\r\n        <h4 class=\"sidebar-info\">Matched: <span>{{ matched }}</span></h4>\r\n        <h4 class=\"sidebar-info\">Attempts: <span>{{ attempts }}</span></h4>\r\n      </div>\r\n\r\n      <div v-if=\"isRanking\">\r\n        <input class=\"sidebar-input d-block\" type=\"text\" v-model=\"playerName\"/>\r\n        <button class=\"action-btn d-block\"\r\n          @click=\"insertScore({ player: playerName, score: playerScore, isPlayer: true })\">\r\n          Insert Score\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapGetters, mapActions } from 'vuex';\r\nimport TimerComp from '@/components/timer.vue';\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    TimerComp,\r\n  },\r\n  data() {\r\n    return {\r\n      rendered: true,\r\n      isRanking: false,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      paused: (state) => state.paused,\r\n      playing: (state) => state.playing,\r\n      attempts: (state) => state.attempts,\r\n      matched: (state) => state.matched,\r\n      completed: (state) => state.completed,\r\n    }),\r\n    ...mapGetters(['attempts', 'matched', 'completed', 'playerScore']),\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      'startGame', 'resumeGame', 'pauseGame', 'insertScore',\r\n    ]),\r\n    enterScore() {\r\n      this.$router.push('/ranking');\r\n      this.isRanking = true;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped lang=\"scss\">\r\n.sidebar {\r\n  width: 312px;\r\n  height: 100vh;\r\n  background: #000;\r\n  padding: 32px;\r\n\r\n  & .sidebar-heading {\r\n    font-size: 56px;\r\n    font-weight: 900;\r\n    color: white;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    line-height: 49px;\r\n  }\r\n\r\n  & .sidebar-info {\r\n    font-size: 18px;\r\n    font-weight: 600;\r\n    color: white;\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    margin-top: 24px;\r\n\r\n    & span {\r\n      font-weight: 300;\r\n    }\r\n  }\r\n\r\n  & .sidebar-input {\r\n    background: white;\r\n    min-width: 230px;\r\n    width: 100%;\r\n    height: 40px;\r\n    font-size: 16px;\r\n    padding: 16px;\r\n    color: black;\r\n    margin: 24px 0;\r\n  }\r\n\r\n  & .action-btn {\r\n      background-color: #fff;\r\n      color: #000;\r\n      min-width: 160px;\r\n      padding: 16px 20px;\r\n      font-size: 16px;\r\n      font-weight: 900;\r\n      text-transform: uppercase;\r\n      border-radius: 8px;\r\n      text-align: center;\r\n      transition: all .2s;\r\n      display: block;\r\n\r\n    &:hover {\r\n      border-radius: 64px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}